{
    "name": "app",
    "private": true,
    "version": "0.0.1-canary",
    "license": "MIT",
    "scripts": {
        "start": "yarn native:start",
        "start:watch": "yarn native:build:pack && concurrently \"yarn renderer:start\" \"yarn native:build:watch\"",
        "native:start": "yarn workspace native start",
        "native:build:watch": "yarn workspace native build:watch",
        "native:build:pack": "yarn workspace native build:pack",
        "native:rebuild-sqlite3": "yarn workspace native rebuild-sqlite3",
        "renderer:start": "fastpack dev",
        "renderer:build": "cross-env NODE_ENV=production fastpack build",
        "renderer:lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
        "renderer:test": "jest",
        "release:build": "node .scripts/clean.js && yarn native:build:pack && yarn renderer:build",
        "release:make:window:win32x64": "cross-env ELECTRON_ZIP_NAME=electron-v18.1.0-win32-x64.zip node .scripts/release.binary.js",
        "release:make:window:linux-x64": "cross-env ELECTRON_ZIP_NAME=electron-v18.1.0-linux-x64.zip node .scripts/release.binary.js"

    },
    "workspaces": [
        "native"
    ],
    "dependencies": {
        "@weblif/react-top-loading-bar": "^2.0.1",
        "date-fns": "^2.28.0",
        "electron": "^18.1.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-icons": "^4.3.1",
        "react-router-dom": "^6.3.0",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/styled-components": "^5.1.25",
        "@weblif/configure": "^0.0.2",
        "@weblif/fastpack": "^1.11.1-canary",
        "@weblif/plugin-less-loader": "^2.3.9",
        "ansi-colors": "^4.1.1",
        "cli-progress": "^3.10.0",
        "concurrently": "^7.1.0",
        "cross-env": "^7.0.3",
        "eslint-webpack-plugin": "^3.0.1",
        "fs-extra": "^10.1.0",
        "jszip": "^3.9.1",
        "rcedit": "^3.0.1"
    }
}
