{
    "name": "app",
    "private": true,
    "version": "0.0.1-canary",
    "license": "MIT",
    "scripts": {
        "start": "concurrently \"yarn renderer:start\" \"yarn native:build:watch\" \"yarn native:start\"",
        "native:start": "yarn workspace native start",
        "native:build:watch": "yarn workspace native build:watch",
        "native:build:pack": "yarn workspace native build:pack",
        "renderer:start": "fastpack dev",
        "renderer:build": "cross-env fastpack build",
        "renderer:lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
        "renderer:test": "jest",
        "release": "node .scripts/clean.js && yarn native:build:pack && yarn renderer:build && node .scripts/release.binary.js"
    },
    "workspaces": [
        "native"
    ],
    "dependencies": {
        "@weblif/react-top-loading-bar": "^2.0.1",
        "axios": "^0.26.1",
        "date-fns": "^2.28.0",
        "dexie": "^3.0.3",
        "electron": "^18.1.0",
        "pinyin": "^2.10.2",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-icons": "^4.3.1",
        "react-router-dom": "^5.3.0",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@types/pinyin": "^2.10.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/react-router-dom": "^5.3.0",
        "@types/styled-components": "^5.1.25",
        "@weblif/configure": "^0.0.2",
        "@weblif/fastpack": "^1.9.12-canary",
        "@weblif/plugin-less-loader": "^2.3.9",
        "concurrently": "^7.1.0",
        "cross-env": "^7.0.3",
        "eslint-webpack-plugin": "^3.0.1",
        "fs-extra": "^10.1.0"
    }
}
